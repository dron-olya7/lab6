public class example2 {
	 private static int count = 0;
	    
	    public static void getCount()
	    {
	        System.out.println(count++);
	    }
	    
	    public static void main(String[] args)
	    {
	        example2.getCount();
	        example2.getCount();
	        example2.getCount();
	    }
	    
	}
